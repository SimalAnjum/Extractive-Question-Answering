16:53:00.245801Z"}},"outputs":[],"execution_count":3},{"cell_type":"markdown","source":"BERT","metadata":{}},{"cell_type":"code","source":"# Load SQuAD v2 dataset\ndataset = load_dataset(\"squad_v2\")\n\n# Load tokenizer and model\nmodel_checkpoint = \"bert-base-uncased\" \ntokenizer = BertTokenizerFast.from_pretrained(model_checkpoint)\nmodel = BertForQuestionAnswering.from_pretrained(model_checkpoint)\n\n# Use a subset of the dataset for faster training/testing\nanswerable = dataset[\"train\"].filter(lambda example: len(example[\"answers\"][\"text\"]) > 0)\nanswerable = answerable.select(range(3000))\nunanswerable = dataset[\"train\"].filter(lambda example: len(example[\"answers\"][\"text\"]) == 0).select(range(2000))\n\ntrain_dataset = concatenate_datasets([answerable, unanswerable])\nvalidation_dataset = dataset[\"validation\"].shuffle(seed=42).select(range(200))  # Subset: 200 samples\n\n# Preprocessing function\ndef preprocess_examples(examples):\n    questions = [q.strip() for q in examples[\"question\"]]\n    inputs = tokenizer(\n        questions,\n        examples[\"context\"],\n        max_length=384,\n        truncation=True,\n        padding=\"max_length\",\n        stride=128,\n        return_overflowing_tokens=True,\n        return_offsets_mapping=True\n    )\n    offset_mapping = inputs.pop(\"offset_mapping\")\n    sample_mapping = inputs.pop(\"overflow_to_sample_mapping\")\n    start_positions = []\n    end_positions = []\n\n    for i, offsets in enumerate(offset_mapping):\n        sample_idx = sample_mapping[i]\n        answers = examples[\"answers\"][sample_idx]\n        if len(answers[\"answer_start\"]) == 0:\n            start_positions.append(0)\n            end_positions.append(0)\n        else:\n            start_char = answers[\"answer_start\"][0]\n            end_char = start_char + len(answers[\"text\"][0])\n            sequence_ids = inputs.sequence_ids(i)\n\n            context_start = sequence_ids.index(1)\n            context_end = len(sequence_ids) - sequence_ids[::-1].index(1) - 1\n\n            if start_char < offsets[context_start][0] or end_char > offsets[context_end][1]:\n                start_positions.append(0)\n                end_positions.append(0)\n            else:\n                start_positions.append(next(idx for idx, offset in enumerate(offsets) if offset[0] > start_char) - 1)\n                end_positions.append(next(idx for idx, offset in enumerate(offsets) if offset[1] >= end_char) - 1)\n\n    inputs[\"start_positions\"] = start_positions\n    inputs[\"end_positions\"] = end_positions\n    return inputs\n\n# Apply preprocessing\ntokenized_train = train_dataset.map(preprocess_examples, batched=True, remove_columns=train_dataset.column_names)\ntokenized_validation = validation_dataset.map(preprocess_examples, batched=True, remove_columns=validation_dataset.column_names)\n\n# Convert datasets to PyTorch format\ntokenized_train.set_format(\"torch\")\ntokenized_validation.set_format(\"torch\")\n\n# Create DataLoaders\ntrain_dataloader = DataLoader(tokenized_train, batch_size=16, shuffle=True)\nvalidation_dataloader = DataLoader(tokenized_validation, batch_size=16)\n\n# Set up optimizer, scheduler, and device\noptimizer = AdamW(model.parameters(), lr=2e-5)\nnum_training_steps = len(train_dataloader) * 3\nlr_scheduler = get_scheduler(\"linear\", optimizer=optimizer, num_warmup_steps=0, num_training_steps=num_training_steps)\ndevice = torch.device(\"cuda\") if torch.cuda.is_available() else torch.device(\"cpu\")\nmodel.to(device)\n\n# Training loop\nepochs = 6\nprogress_bar = tqdm(range(num_training_steps))\n\nfor epoch in range(epochs):\n    model.train()\n    \n    # Initialize variables for training metrics\n    total_train_loss = 0\n    correct_train = 0\n    total_train = 0\n    \n    for batch in train_dataloader:\n        batch = {k: v.to(device) for k, v in batch.items()}\n        \n        # Forward pass\n        outputs = model(**batch)\n        loss = outputs.loss\n        start_logits = outputs.start_logits\n        end_logits = outputs.end_logits\n        \n        # Backward pass and optimization\n        optimizer.zero_grad()\n        loss.backward()\n        optimizer.step()\n        lr_scheduler.step()\n        \n        # Update training loss\n        total_train_loss += loss.item()\n        \n        # Calculate training accuracy\n        for i in range(batch[\"input_ids\"].shape[0]):\n            start_idx = torch.argmax(start_logits[i]).item()\n            end_idx = torch.argmax(end_logits[i]).item()\n            # Check if the prediction is correct (very simple check)\n            true_start = batch[\"start_positions\"][i].item()\n            true_end = batch[\"end_positions\"][i].item()\n            if start_idx == true_start and end_idx == true_end:\n                correct_train += 1\n            total_train += 1\n        \n        progress_bar.update(1)\n\n    # Calculate average training loss and accuracy\n    avg_train_loss = total_train_loss / len(train_dataloader)\n    train_accuracy = correct_train / total_train * 100  # In percentage\n\n    # Evaluation phase\n    model.eval()\n    total_val_loss = 0\n    correct_val = 0\n    total_val = 0\n    all_predictions = []\n    all_references = []\n    \n    with torch.no_grad():\n        for batch in validation_dataloader:\n            batch = {k: v.to(device) for k, v in batch.items()}\n            outputs = model(**batch)\n            loss = outputs.loss\n            start_logits = outputs.start_logits\n            end_logits = outputs.end_logits\n            \n            # Update validation loss\n            total_val_loss += loss.item()\n            \n            # Calculate validation accuracy\n            for i in range(batch[\"input_ids\"].shape[0]):\n                start_idx = torch.argmax(start_logits[i]).item()\n                end_idx = torch.argmax(end_logits[i]).item()\n                true_start = batch[\"start_positions\"][i].item()\n                true_end = batch[\"end_positions\"][i].item()\n                if start_idx == true_start and end_idx == true_end:\n                    correct_val += 1\n                total_val += 1\n                \n                # Decode predictions\n                for i, input_ids in enumerate(batch[\"input_ids\"]):\n                    pred_answer = tokenizer.decode(input_ids[start_idx:end_idx + 1], skip_special_tokens=True)\n                    all_predictions.append(pred_answer)\n\n    # Calculate average validation loss and accuracy\n    avg_val_loss = total_val_loss / len(validation_dataloader)\n    val_accuracy = correct_val / total_val * 100  # In percentage\n\n    # Print results for the current epoch\n    print(f\"Epoch {epoch + 1} complete.\")\n    print(f\"Training Loss: {avg_train_loss:.4f}\")\n    print(f\"Training Accuracy: {train_accuracy:.2f}%\")\n    print(f\"Validation Loss: {avg_val_loss:.4f}\")\n    print(f\"Validation Accuracy: {val_accuracy:.2f}%\")\n    print(f\"Example Predictions: {all_predictions[:5]}\")  # Display a few predictions\n\n# Save the model locally\nmodel.save_pretrained(\"./bert-squad-v2\")\ntokenizer.save_pretrained(\"./bert-squad-v2\")\nprint(\"Model saved locally to './bert-squad-v2'.\")\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-04T16:53:08.308938Z","iopub.execute_input":"2024-12-04T16:53:08.309570Z","iopub.status.idle":"2024-12-04T17:30:58.212397Z","shell.execute_reply.started":"2024-12-04T16:53:08.309534Z","shell.execute_reply":"2024-12-04T17:30:58.211498Z"}},"outputs":[{"output_type":"display_data","data":{"text/plain":"README.md:   0%|          | 0.00/8.92k [00:00<?, ?B/s]","application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"13b2bf9694fd4d4ea44fa4b345c69292"}},"metadata":{}},{"output_type":"display_data","data":{"text/plain":"train-00000-of-00001.parquet:   0%|          | 0.00/16.4M [00:00<?, ?B/s]","application/vnd.jupyter.widget-view+json":{"version_major":2,