mmon) == 0:\n            f1 = 0\n        else:\n            precision = len(common) / len(pred_tokens)\n            recall = len(common) / len(true_tokens)\n            f1 = 2 * (precision * recall) / (precision + recall)\n        max_f1 = max(max_f1, f1)\n    return max_f1, exact_match\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-04T17:34:32.842137Z","iopub.execute_input":"2024-12-04T17:34:32.842861Z","iopub.status.idle":"2024-12-04T17:34:32.849500Z","shell.execute_reply.started":"2024-12-04T17:34:32.842826Z","shell.execute_reply":"2024-12-04T17:34:32.848631Z"}},"outputs":[],"execution_count":6},{"cell_type":"code","source":"def evaluate_model(model, dataloader, dataset, tokenizer):\n    model.eval()\n    total_f1 = 0\n    total_em = 0\n    count = 0\n\n    for batch in tqdm(dataloader, desc=\"Evaluating\"):\n        batch = {k: v.to(device) for k, v in batch.items()}\n        with torch.no_grad():\n            outputs = model(**batch)\n        start_logits = outputs.start_logits\n        end_logits = outputs.end_logits\n\n        for i, input_ids in enumerate(batch[\"input_ids\"]):\n            # Get predicted answer\n            start_idx = torch.argmax(start_logits[i]).item()\n            end_idx = torch.argmax(end_logits[i]).item()\n            pred_answer = tokenizer.decode(input_ids[start_idx:end_idx + 1], skip_special_tokens=True)\n\n            # Get true answers\n            true_answers = dataset[\"answers\"][i][\"text\"]\n\n            # Compute F1 and exact match\n            f1, em = compute_f1_and_exact(pred_answer, true_answers)\n            total_f1 += f1\n            total_em += em\n            count += 1\n\n    avg_f1 = total_f1 / count\n    avg_em = total_em / count\n\n    print(f\"F1 Score: {avg_f1:.4f}\")\n    print(f\"Exact Match (EM): {avg_em:.4f}\")\n    return avg_f1, avg_em\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-04T17:34:39.131801Z","iopub.execute_input":"2024-12-04T17:34:39.132616Z","iopub.status.idle":"2024-12-04T17:34:39.139236Z","shell.execute_reply.started":"2024-12-04T17:34:39.132576Z","shell.execute_reply":"2024-12-04T17:34:39.138391Z"}},"outputs":[],"execution_count":8},{"cell_type":"code","source":"# Evaluate after training\n#LOAD saved model\nmodel = BertForQuestionAnswering.from_pretrained(\"./bert-squad-v2\")\nmodel.to(device)\navg_f1, avg_em = evaluate_model(model, validation_dataloader, validation_dataset, tokenizer)\nprint(f\"Final F1 Score: {avg_f1:.4f}\")\nprint(f\"Final Exact Match (EM): {avg_em:.4f}\")\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-04T17:34:42.004722Z","iopub.execute_input":"2024-12-04T17:34:42.005485Z","iopub.status.idle":"2024-12-04T17:34:47.153803Z","shell.execute_reply.started":"2024-12-04T17:34:42.005449Z","shell.execute_reply":"2024-12-04T17:34:47.152885Z"}},"outputs":[{"name":"stderr","text":"\nEvaluating:   0%|          | 0/13 [00:00<?, ?it/s]\u001b[A\nEvaluating:   8%|▊         | 1/13 [00:00<00:05,  2.36it/s]\u001b[A\nEvaluating:  15%|█▌        | 2/13 [00:00<00:04,  2.50it/s]\u001b[A\nEvaluating:  23%|██▎       | 3/13 [00:01<00:03,  2.55it/s]\u001b[A\nEvaluating:  31%|███       | 4/13 [00:01<00:03,  2.57it/s]\u001b[A\nEvaluating:  38%|███▊      | 5/13 [00:01<00:03,  2.59it/s]\u001b[A\nEvaluating:  46%|████▌     | 6/13 [00:02<00:02,  2.60it/s]\u001b[A\nEvaluating:  54%|█████▍    | 7/13 [00:02<00:02,  2.59it/s]\u001b[A\nEvaluating:  62%|██████▏   | 8/13 [00:03<00:01,  2.60it/s]\u001b[A\nEvaluating:  69%|██████▉   | 9/13 [00:03<00:01,  2.60it/s]\u001b[A\nEvaluating:  77%|███████▋  | 10/13 [00:03<00:01,  2.60it/s]\u001b[A\nEvaluating:  85%|████████▍ | 11/13 [00:04<00:00,  2.59it/s]\u001b[A\nEvaluating:  92%|█████████▏| 12/13 [00:04<00:00,  2.59it/s]\u001b[A\nEvaluating: 100%|██████████| 13/13 [00:04<00:00,  2.63it/s]\u001b[A","output_type":"stream"},{"name":"stdout","text":"F1 Score: 0.6086\nExact Match (EM): 0.6078\nFinal F1 Score: 0.6086\nFinal Exact Match (EM): 0.6078\n","output_type":"stream"},{"name":"stderr","text":"\n","output_type":"stream"}],"execution_count":9},{"cell_type":"markdown","source":"BERT - Trivia QA","metadata":{}},{"cell_type":"code","source":"from transformers import AutoTokenizer, AutoModelForQuestionAnswering\nfrom datasets import load_dataset\nimport torch\nfrom tqdm import tqdm\nimport numpy as np\n\n# Load the TriviaQA dataset validation set\ndataset = load_dataset(\"trivia_qa\", \"unfiltered\")  # Load only 200 samples\ndataset = dataset[\"validation\"].select(range(200))\nprint(dataset[0])\n\nmodel_name_or_path = \"./bert-squad-v2\"\ntokenizer = AutoTokenizer.from_pretrained(model_name_or_path)\nmodel = AutoModelForQuestionAnswering.from_pretrained(model_name_or_path)\n\n# Function to compute Exact Match (EM) and F1 score\ndef compute_em_and_f1(prediction, ground_truths):\n    def normalize_answer(s):\n        \"\"\"Lower text and remove punctuation, articles, and extra whitespace.\"\"\"\n        import string, re\n\n        def remove_articles(text):\n            return re.sub(r\"\\b(a|an|the)\\b\", \" \", text)\n\n        def white_space_fix(text):\n            return \" \".join(text.split())\n\n        def remove_punctuation(text):\n            return \"\".join(ch for ch in text if ch not in string.punctuation)\n\n        def lower(text):\n            return text.lower()\n\n        return white_space_fix(remove_articles(remove_punctuation(lower(s))))\n\n    def f1_score(prediction, ground_truth):\n        prediction_tokens = normalize_answer(prediction).split()\n        ground_truth_tokens = normalize_answer(ground_truth).split()\n        common = set(prediction_tokens) & set(ground_truth_tokens)\n        num_same = len(common)\n        if len(prediction_tokens) == 0 or len(ground_truth_tokens) == 0:\n            return int(prediction_tokens == ground_truth_tokens)\n        if num_same == 0:\n            return 0\n        precision = num_same / len(prediction_tokens)\n        recall = num_same / len(ground_truth_tokens)\n        return (2 * precision * recall) / (precision + recall)\n\n    em = 0\n    f1 = 0\n    for ground_truth in ground_truths:\n        em = max(em, normalize_answer(prediction) == normalize_answer(ground_truth))\n        f1 = max(f1, f1_score(prediction, ground_truth))\n    return em, f1\n\ndef evaluate(model, tokenizer, dataset, max_len=384, stride=128):\n    model.eval()\n    em_scores = []\n    f1_scores = []\n\n    for sample in tqdm(dataset):\n        question = sample[\"question\"]\n\n        # Extract and concatenate descriptions as context\n        if \"search_results\" in sample and \"description\" in sample[\"search_results\"]:\n            context = \" \".join(sample[\"search_results\"][\"description\"])\n        else:\n            continue  # Skip samples with no context\n\n        ground_truths = sample[\"answer\"][\"aliases\"] if \"answer\" in sample else []\n\n                # Tokenize input\n        inputs = tokenizer(\n            question,\n            context,\n            max_length=max_len,\n            stride=stride,\n            truncation=True,\n            return_overflowing_tokens=False,  # Disable this to avoid multi-slice input\n            return_offsets_mapping=True,\n            padding=\"max_length\",\n        )\n        input_ids = torch.tensor(inputs[\"input_ids\"]).unsqueeze(0)  # Add batch dimension\n        attention_mask = torch.tensor(inputs[\"attention_mask\"]).unsqueeze(0)  # Add batch dimension\n\n        with torch.no_grad():\n            outputs = model(input_ids, attention_mask=attention_mask)\n            start_logits, end_logits = outputs.start_logits, outputs.end_logits\n\n        # Get most probable start and end of the answer span\n        start_idx = torch.argmax(start_logits, dim=-1).item()\n        end_idx = torch.argmax(end_logits, dim=-1).item()\n\n        # Decode the answer\n        predicted_answer = tokenizer.decode(input_ids[0, start_idx:end_idx + 1])\n\n        # Compute EM and F1\n        em, f1 = compute_em_and_f1(predicted_answer, ground_truths)\n        em_scores.append(em)\n        f1_scores.append(f1)\n\n    # Compute average EM and F1 scores\n    avg_em = np.mean(em_scores) if em_scores else 0\n    avg_f1 = np.mean(f1_scores) if f1_scores else 0\n    return avg_em, avg_f1\n\n\n# Run evaluation\navg_em, avg_f1 = evaluate(model, tokenizer, dataset)\n\nprint(f\"Evaluation Results on 200 samples:\")\nprint(f\"Exact Match (EM): {avg_em * 100:.2f}%\")\nprint(f\"F1 Score: {avg_f1 * 100:.2f}%\")\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-04T17:35:12.452661Z","iopub.execute_input":"2024-12-04T17:35:12.453012Z","iopub.status.idle":"2024-12-04T17:41:26.435474Z","shell.execute_reply.started":"2024-12-04T17:35:12.452981Z","shell.execute_reply":"2024-12-04T17:41:26.434485Z"}},"outputs":[{"output_type":"display_data","data":{"text/plain":"README.md:   0%|          | 0.00/26.7k [00:00<?, ?B/s]","application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"dbab4bea80564e98a0202b5f2b9fcc7c"}},"metadata":{}},{"output_type":"display_data","data":{"text/plain":"Resolving data files:   0%|          | 0/26 [00:00<?, ?it/s]","application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"24ce1f9d0dc64a60af4ddee002fcd334"}},"metadata":{}},{"output_type":"display_data","data":{"text/plain":"Resolving data files:   0%|          | 0/47 [00:00<?, ?it/s]","application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"a6f5bce53bed4a3e83e4d5629fdc3565"}},"metadata":{}},{"output_type":"display_data","data":{"text/plain":"Downloading data:   0%|          | 0/47 [00:00<?, ?files/s]","application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"331bb04e012548d18b609d4d7061008d"}},"metadata":{}},{"output_type":"display_data","data":{"text/plain":"train-00000-of-00047.parquet:   0%|          | 0.00/215M [00:00<?, ?B/s]","application